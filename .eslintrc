{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "no-console": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-mixed-operators": 0,
    "default-case": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "newline-per-chained-call": 0
  }
}
